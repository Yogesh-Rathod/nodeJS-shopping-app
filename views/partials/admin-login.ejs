<div class="container-fluid">
	<div class="jumbotron">
		<h1 class="h1">Welcome Admin! Please Login</h1>
	</div>

	<%- messages() %>

	<div class="col-xs-3 col-xs-offset-4">
		<form action="/adminLogin" method="POST" >
			<div class="form-group row">
				<label for="example-text-input" class="col-2 col-form-label">Enter Username (admin)</label>
				<div class="col-10">
					<input class="form-control" type="text" value="" id="example-text-input" name="username">
				</div>
			</div>
			<div class="form-group row">
				<label for="example-search-input" class="col-2 col-form-label">Enter Password (admin)</label>
				<div class="col-10">
					<input class="form-control" type="password" value="" id="" name="password">
				</div>
			</div>
			<div class="form-group row">
				<div class="col-10">
					<input type="submit" name="submit" value="Submit" class="btn btn-submit">
				</div>
			</div>
		</form>
	</div>